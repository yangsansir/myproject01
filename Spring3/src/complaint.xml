<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
   <package name="complaint"  namespace="/complaint"  extends="default1">
 
    <action name="add" class="com.action.ComplaintAction" method="add">
   <result type="redirectAction">findAll</result>
   </action>
    <action name="findAll" class="com.action.ComplaintAction" method="findAll">
   <result name="success">/complaint/AllComplaint.jsp</result>
   </action>
    <action name="delete" class="com.action.ComplaintAction" method="delete">
   <result type="redirectAction">findAll</result>
   </action>
    </package> 
</struts>
