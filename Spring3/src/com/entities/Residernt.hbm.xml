<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="Residernt" table="residernt">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="LoginName" type="java.lang.String" column="loginName"/>
	     <property name="Password" type="java.lang.String" column="password"/>
	     <property name="RealName" type="java.lang.String" column="realName"/>
	     <property name="RoomNumber" type="java.lang.String" column="roomNumber"/>
	     <property name="Telephone" type="java.lang.String" column="telephone"/>
	       <many-to-one name="middleFloor"  column="middleFloor_id" class="MiddleFloor" lazy="false"></many-to-one>
     </class>

</hibernate-mapping>