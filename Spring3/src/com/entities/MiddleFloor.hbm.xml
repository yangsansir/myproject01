<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="MiddleFloor" table="middleFloor">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="BuildNUmber" type="java.lang.String" column="buildNUmber"/>
	     <set name="residerntSet" lazy="false">
	     <key column="middleFloor_id" ></key>
	     <one-to-many class="Residernt"/>
	     </set>
	     <many-to-one name="district" column="district_id" class="District" lazy="false"></many-to-one>
     </class>

</hibernate-mapping>