<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
   <package name="repairType"  namespace="/repairType"  extends="default1">
    <action name="add" class="com.action.RepairTypeAction" method="add">
 <result name="success" type="redirectAction">findAll</result>
   </action>
    <action name="findAll" class="com.action.RepairTypeAction" method="findAll">
   <result name="success">/repairType/AllRepairType.jsp</result>
   </action>
  
    <action name="delete" class="com.action.RepairTypeAction" method="delete">
   <result name="success" type="redirectAction">findAll</result>
   </action>
    <action name="update" class="com.action.RepairTypeAction" method="update">
   <result name="success" type="redirectAction">findAll</result>
   </action>
     <action name="findById" class="com.action.RepairTypeAction" method="findById">
   <result name="success">/repairType/UpdateRepairType.jsp</result>
   </action>
   
    </package> 
</struts>
