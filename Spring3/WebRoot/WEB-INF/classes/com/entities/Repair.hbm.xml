<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
      <hibernate-mapping package="com.entities">
	  <class name="Repair" table="repair">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	       <property name="CreateTime" type="java.util.Date" column="createTime"/>
	       <property name="RepairMessages" type="java.lang.String" column="repairMessages"/>
	       <property name="contact" type="java.lang.String" column="contact"/>
	        <property name="telephone" type="java.lang.String" column="telephone"/>
	         <property name="status" type="java.lang.String" column="status"/> 
	  <many-to-one name="repairType" column="repairType_id" class="RepairType"></many-to-one>
     </class>

</hibernate-mapping>