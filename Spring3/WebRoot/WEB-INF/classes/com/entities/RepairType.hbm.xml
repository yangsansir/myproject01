<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="RepairType" table="repairType">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="RepairType" type="java.lang.String" column="repairType"/>
	     <set name="repairSet">
	     <key column="repairType_id"></key>
	     <one-to-many class="Repair"/>
	     </set>
     </class>

</hibernate-mapping>