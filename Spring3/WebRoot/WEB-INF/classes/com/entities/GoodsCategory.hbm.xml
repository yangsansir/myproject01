<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="GoodsCategory" table="goodsCategory">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="categoryName" type="java.lang.String" column="categoryName"/>
	    <property name="categoryDescription"  column="categoryDescription"/>
	   <set name="goods" lazy="false" >
	     <key column="goodsCategory_id"></key>
	     <one-to-many class="Goods"/>
	     </set>
     </class>

</hibernate-mapping>