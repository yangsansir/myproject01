<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="Function" table="function">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="FunctionName" type="java.lang.String" column="functionName"/>
	       <property name="Url" type="java.lang.String" column="url"/>
	     <many-to-one name="role" column="role_id" class="Role"></many-to-one>
     </class>

</hibernate-mapping>