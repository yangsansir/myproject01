<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="CheckUser" table="CheckUser">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="checkUserNmae" type="java.lang.String" column="checkUserNmae"/>
	      <set name="carPaymentSet">
	  <key column="checkUser_id"></key>
	  <one-to-many class="CarPayment"/>
	  </set>
	    <set name="billOutBodySet">
	  <key column="checkUser_id"></key>
	  <one-to-many class="BillOutBody"/>
	  </set>
	  <set name="singlePaymentSet">
	  <key column="checkUser_id"></key>
	  <one-to-many class="SinglePayment"/>
	  </set>
     </class>

</hibernate-mapping>