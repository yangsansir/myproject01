<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="Salary" table="salary">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	   
	    <property name="RealName" type="java.lang.String" column="RealName"/>
	      <property name="Insurance"  column="insurance"/>
	        <property name="AccumlationFund"  column="accumlationFund"/>
	       <property name="Individual"  column="individual"/>
	         <property name="Bonus"  column="bonus"/>
	           <property name="Replacement"  column="replacement"/>
	             <property name="Realhair"  column="realhair"/>
	   <property name="status" type="java.lang.String" column="status"/>
     </class>

</hibernate-mapping>