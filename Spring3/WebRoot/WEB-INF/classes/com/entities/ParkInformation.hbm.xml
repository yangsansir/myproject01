<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="ParkInformation" table="parkInformation">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="OwnerName" type="java.lang.String" column="OwnerName"/>
	      <property name="LicensePlate" type="java.lang.String" column="LicensePlate"/>
	       <property name="location" type="java.lang.String" column="location"/>
	         <property name="districtName" type="java.lang.String" column="districtName"/>
	         <many-to-one name="carType" column="carType_id" class="CarType" lazy="false"></many-to-one>
     </class>

</hibernate-mapping>