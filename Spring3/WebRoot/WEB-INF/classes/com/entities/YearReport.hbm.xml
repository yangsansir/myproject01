<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="YearReport" table="yearReport">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="createTime" type="java.util.Date" column="createTime"/>
	     <property name="yearPay" type="java.lang.String" column="yearPay"/>
	      <property name="yearIncome" type="java.lang.String" column="yearIncome"/>
	       <property name="year"  column="year"/>
	        <property name="yearPayCount"  column="yearPayCount"/>
	         <property name="yearIncomeCount"  column="yearIncomeCount"/>
	    
	     <many-to-one name="district" column="district_id" class="District" lazy="false"></many-to-one>
     </class>

</hibernate-mapping>