<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.entities">
	 <class name="Warehouse" table="warehouse">
		<id name="id" column="id">
            <generator class="native"/>
        </id>
	     <property name="warehouseName" type="java.lang.String" column="warehouseName"/>
	      <property name="pyCode" type="java.lang.String" column="pyCode"/>
	       <property name="location" type="java.lang.String" column="location"/>
	         <property name="description" type="java.lang.String" column="description"/>
	    
     </class>

</hibernate-mapping>